{
  "name": "comfyui-browser",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "extract": "svelte-i18n extract './src/**/*.svelte' ./src/i18n/zh-CN.json && svelte-i18n extract './src/**/*.svelte' ./src/i18n/en-US.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.22",
    "postcss": "^8.4.32",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "type": "module",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "dayjs": "^1.11.12",
    "dexie": "^4.0.8",
    "pubsub-js": "^1.9.4",
    "svelte-i18n": "^4.0.0",
    "uuid": "^10.0.0"
  }
}
